Page {
  background: #f2f2f2;
}

.title_fixed {
  position: fixed;
  z-index: 999;
  background: #fff;
  width: 750rpx;
}

.title_box {
  margin-left: 25rpx;
  margin-top: 20rpx;
  margin-bottom: 20rpx;
  height: 200rpx;
  width: 700rpx;
  display: flex;
  display: -webkit-flex;
  justify-content: space-around;
  align-items: center;
  border-radius: 10rpx;
  box-shadow: 0rpx 0rpx 4rpx 4rpx #efefef;
  background: #fff;
}

.title_left {
  width: 360rpx;
}

.title_left .price {
  font-size: 40rpx;
  line-height: 80rpx;
  text-align: center;
  color: #f55;
  font-weight: 800;
}

.title_left .text {
  font-size: 25rpx;
  line-height: 40rpx;
  text-align: center;
  color: #ccc;
}

.title_right {
  
  width: 360rpx;
  border-left: 2rpx solid #efefef;
  text-align: center;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
}

.invis_btn {
  position: relative;
  padding-left: 20rpx;
  padding-right: 65rpx;
  display: inline-block;
  line-height: 80rpx;
  text-align: center;
  color: #fff;
  background: -webkit-linear-gradient(left, rgb(242, 63, 49), rgb(228, 133, 5));
  font-size: 30rpx;
  border-radius: 10rpx;
  box-shadow: 0px 0px 4rpx #963c3c;
}
.button-hover{
  opacity: 0.8;
}
.btn_icon {
  width: 45rpx;
  display: inline-block;
  position: absolute;
  top: 50%;
  margin-top: -30rpx;
  right: 10rpx;
}

.btn_icon image {
  width: 100%;
}

.tabBar {
  display: flex;
  justify-content: space-around;
  align-items: center;
  width: 750rpx;
  background: #fff;
}

.bar_inner {
  padding-left: 20rpx;
  padding-right: 20rpx;
  font-size: 30rpx;
  line-height: 90rpx;
}

.hover {
  border-bottom: 4rpx solid #f55;
  color: #f55;
  font-size: 32rpx;
  font-weight: 800;
}

.normal {
  color: #aeaeae;
  font-size: 28rpx;
}

/* 列表 */

.columns {
  background: #fff;
  width: 710rpx;
  padding: 20rpx;
  margin-top: 10rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.columns_left {
  width: 200rpx;
  height: 200rpx;
}

.columns_left image {
  width: 200rpx;
  height: 200rpx;
  border-radius: 10rpx;
}

.columns_right {
  width: 490rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
  flex-wrap: wrap;
}

.columns_title {
  width: 490rpx;
  line-height: 45rpx;
  font-size: 30rpx;
  max-height: 100rpx;
  color: #333;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  word-break: break-all;
}

.columns_price_box {
  width: 300rpx;
}

.columns_normal_price {
  color: #aeaeae;
  font-size: 26rpx;
  line-height: 40rpx;
  text-decoration: line-through;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
}

.columns_price {
  color: #f55;
  font-size: 30rpx;
  font-weight: 800;
  overflow: hidden;
  text-overflow: ellipsis;
  white-space: nowrap;
  line-height: 60rpx;
}

.btn_box {
  width: 180rpx;
  margin-top: 10rpx;
}

.share_btn {
  width: 170rpx;
  line-height: 60rpx;
  color: #fff;
  border-radius: 10rpx;
  text-align: center;
  font-size: 28rpx;
}

.share_text {
  color: #aeaeae;
  font-size: 25rpx;
  line-height: 50rpx;
  text-align: center;
}

/* 佣金收入 */

.list_box {
  background: #fff;
  margin-top: 10rpx;
}

.inner_box {
  width: 710rpx;
  margin: 0 auto;
  padding-top: 20rpx;
  padding-bottom: 20rpx;
  display: flex;
  justify-content: space-around;
  align-items: center;
}

.inner_left {
  width: 200rpx;
  height: 200rpx;
  border-radius: 10rpx;
  overflow: hidden;
}

.inner_left image {
  width: 200rpx;
  height: 200rpx;
}

.inner_center {
  width: 350rpx;
  padding-left: 10rpx;
}

.cargoTitle {
  font-size: 28rpx;
  line-height: 45rpx;
  overflow: hidden;
  text-overflow: ellipsis;
  display: -webkit-box;
  -webkit-line-clamp: 2;
  -webkit-box-orient: vertical;
  word-break: break-all;
  color: #333;
  max-height: 90rpx;
}

.cargoSpec {
  font-size: 25rpx;
  line-height: 60rpx;
  text-overflow: ellipsis;
  overflow: hidden;
  white-space: nowrap;
  height: 45rpx;
  color: #aeaeae;
}

.inner_right {
  width: 150rpx;
}

.income_time {
  font-size: 25rpx;
  color: #aeaeae;
  line-height: 60rpx;
  text-align: left;
}

.income_icon {
  width: 80rpx;
  height: 80rpx;
  margin: 0 auto;
}

.income_icon image {
  width: 80rpx;
  height: 80rpx;
}

.income {
  font-size: 32rpx;
  color: #f55;
  line-height: 60rpx;
  text-align: center;
  font-weight: 800;
}

.isLoading {
  font-size: 30rpx;
  color: #aeaeae;
  text-align: center;
  line-height: 60rpx;
}

.isLoading text {
  color: #aeaeae;
  font-size: 30rpx;
}

.isLast {
  color: #aeaeae;
  line-height: 60rpx;
  font-size: 30rpx;
  text-align: center;
}
