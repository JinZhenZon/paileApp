    switch (parseInt(status)){
      case 1:return "已完成";break;
      case 2: return "订单失败"; break;
      case 3: return "订单取消"; break;
      case 4: return "创建订单"; break;
      case 5: return "待付款"; break;
      case 6: return "支付中"; break;
      case 7: return "已付款"; break;
      case 8: return "待发货"; break;
      case 9: return "已发货"; break;      
      case 10: return "待收货"; break;
      case 11: return "已收货"; break;
      case 12: return "待退换"; break;
      case 13: return "已退换"; break;
      case 14: return "删除订单"; break;
      case 15: return "待评价"; break;
      case 16: return "已评价"; break;
    }
      
  },

}

module.exports = {
  filterStatus: filter.filterStatus
}