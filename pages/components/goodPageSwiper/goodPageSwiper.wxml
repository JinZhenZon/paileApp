<!--pages/components/goodPageSwiper/goodPageSwiper.wxml-->
<swiper indicator-dots = "true" autoplay='true' interval='4000' circular='true' class = "bowerSlide" >
  <block  wx:for = "{{imgUrl}}" wx:key = "{{index}}">
    <swiper-item>
          <image src = "{{item}}" catchtap="slideShow" data-list = "{{imgUrl}}"  class="slide-image" width="355" height="150" class = "slideImage"></image>
    </swiper-item>
  </block>
</swiper>

